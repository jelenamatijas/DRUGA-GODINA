package net.etfbl.pj2;

import java.util.Scanner;
import java.io.*;


public class CounterThread extends Thread
{
  private String name;
  private int count;
  
  
  public CounterThread (String s,int p)
  {
    name = s;
    count = p;
    setName(name);
  }
  
  public void run()
  {
    int suma = 0;
    for(int i=0;i<this.count;i++) suma+=i;
    
    Console cons = System.console();
  }
  
}
    