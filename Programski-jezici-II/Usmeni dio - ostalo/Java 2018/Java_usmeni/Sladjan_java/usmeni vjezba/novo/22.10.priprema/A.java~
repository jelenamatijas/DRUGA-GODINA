class A {
 static int i;
 A() {
  ++i;
 }

 public int metoda() {
  return ++i;
 }
 
 public static void main(String ka[]) {
  X x = new X();
  A a = new A();
  a = (A) x;
  System.out.println(a.metoda());
 }
};

class B extends A {

 B() {
  i++;
 }

