import java.util.Scanner
import java.util.Arrays
import java.lang.String
import java.util.Random
  
  
public class Klijent 
{
  private String ime;
  private String prezime;
  private int[] JMBG;
  private double stanjeRacuna;
  
  
  public Klijent()
  {
    ime = "N/N";
    prezime = "N/N";
    
    JMBG = new int [13];
    for (int i=0;i<13;i++)
    {
      JMBG[i] = 0;
    }
    
    stanjeRacuna = 0;
  }
  
  
}
  
  