import java.io.*;
import java.net.*;
import java.lang.String;



public class CitacObjekata
{
  public static void main(String[] args)
  {
  try
  {
    ServerSocket ss = new ServerSocket(9000);
    
    int j = 0;
    while(true)
    {
      ss.accept();
      ++j;
      System.out.println("Client " + j + " accepted");
    }
    
  }
  catch(Exception ex)
  {
    ex.printStackTrace();
  }
  }
  
  
  
}