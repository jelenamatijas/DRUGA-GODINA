class C{
 
 static double i=1;
 static int j=2;
 int x=3;   // nije staticka promijenjiva
 static int y=6;
 
 public static void main(String args[]){
  metoda();
  System.out.println(i + j);
//  System.out.println(x + i); // ne moze iz statickog konteksta da se poziva nestaticki clan x
  metoda2();
  System.out.println(i + y);
 // System.out.println(x + j); // ne moze iz statickog konteksta da se poziva nestaticki clan x
 }
 
 public static int metoda(){
  return (int)i + --y + (j++);
 }
 
 public static double metoda2(){
  return j++ - --i;
 }
}